<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(ufptitle:string) -%>">
  <span class='title'>
    <%= ufptitle:string. %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.ufptitle:strings_ufptitle:string_path(ufptitle:string),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_ufptitle:strings_admin_ufptitle:string_path(ufptitle:string),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.ufptitle:strings_admin_ufptitle:string_path(ufptitle:string),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => ufptitle:string.),
        :method => :delete %>
  </span>
</li>
